{
  "name": "scatterplot",
  "version": "0.1.0",
  "description": "Scatterplot Graph Challenge from Free Code Camp",
  "main": "build/index.js",
  "scripts": {
    "clean": "rimraf build/",
    "start": "cross-env NODE_ENV=development nodemon -r babel-register -r babel-polyfill -r dotenv/config server/development.js",
    "build:app": "webpack -p --config ./webpack.production.config.js --progress --color",
    "copyFavicon": "cp favicon.ico ./build/favicon.ico",
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=production npm run build:app && npm run copyFavicon",
    "predeploy": "npm run build",
    "deploy": "rimraf docs/ && cp -r build/ docs/",
    "lint": "eslint app/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/natac13/scatterplot.git"
  },
  "author": "Sean Campbell <sean.campbell13@gmail.com> (https://seancampbellnatac.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/natac13/scatterplot/issues"
  },
  "homepage": "https://github.com/natac13/scatterplot#readme",
